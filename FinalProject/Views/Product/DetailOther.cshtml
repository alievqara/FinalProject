@model OtherProduct



<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-xl-12">
            <            <div class="card">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-striped">
                            <tbody>
                                <tr>
                                    <th> Product Brand </th>
                                    <td> @Model.Brand_Name </td>
                                    <th> Product Model </th>
                                    <td> @Model.Model_Name </td>
                                </tr>
                                <tr>
                                    <th> Product Detail </th>
                                    <td> @Model.Detail </td>
                                    <th> Product Price </th>
                                    <td> $ @Model.Price </td>
                                </tr>
                                <tr>
                                    <th> Product Stock or Sale </th>
                                    <td> @Model.Stock_Sale </td>
                                    <th> Product Deffect </th>
                                    <td> @Model.Yararsiz </td>
                                </tr>
                                <tr>
                                    <th> Product Customer </th>
                                    <td> @Model.UserSale </td>
                                    <th> Product BarCode </th>
                                    <td> @Model.Barcode </td>
                                </tr>
                                <tr>
                                    <th> Product Creator Name</th>
                                    <td> @Model.CreatorName </td>
                                    <th> Product Created Date </th>
                                    <td> @Model.CreatedTime </td>

                                </tr>
                                <tr>

                                    <th> Product Updator Name </th>
                                    <td> @Model.EditorName </td>
                                    <th> Product Updated Time </th>
                                    <td> @Model.EditedTime </td>

                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>