@model Computer



<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-xl-12">
            <            <div class="card">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-striped">
                            <tbody>
                                <tr>
                                    <th> Computer Brand </th>
                                    <td> @Model.Brand_Name </td>
                                    <th> Computer Model </th>
                                    <td> @Model.Model_Name </td>
                                </tr>
                                <tr>
                                    <th> Computer Detail </th>
                                    <td> @Model.Detail </td>
                                    <th> Computer Price </th>
                                    <td> $ @Model.Price </td>
                                </tr>
                                <tr>
                                    <th> Computer Stock or Sale </th>
                                    <td> @Model.Stock_Sale </td>
                                    <th> Computer Deffect </th>
                                    <td> @Model.Yararsiz </td>
                                </tr>
                                <tr>
                                    <th> Computer Customer </th>
                                    <td> @Model.UserSale </td>
                                    <th> Computer S/N Seria </th>
                                    <td> @Model.SN_Seria </td>
                                </tr>
                                <tr>
                                    <th> Computer Ram </th>
                                    <td> @Model.Ram </td>
                                    <th> Computer Storage </th>
                                    <td> @Model.Storage </td>
                                </tr>
                                <tr>
                                    <th> Computer Creator Name</th>
                                    <td> @Model.CreatorName </td>
                                    <th> Computer Created Date </th>
                                    <td> @Model.CreatedTime </td>

                                </tr>
                                <tr>

                                    <th> Computer Updator Name </th>
                                    <td> @Model.EditorName </td>
                                    <th> Computer Updated Time </th>
                                    <td> @Model.EditedTime </td>

                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>